    .#{$prefix}btn-#{$ui}-icon {
        button {
            padding: 0;
            width: $icon-size !important;
            height: $icon-size;
        }

        .#{$prefix}btn-icon {
            width: $icon-size;
            height: $icon-size;
            top: 0;
            left: 0;
            bottom: 0;
            right: 0;
        }
    }
    
    .#{$prefix}btn-#{$ui}-icon-text-left {
